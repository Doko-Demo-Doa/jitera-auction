{
  "name": "jitera-auction",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepare": "husky install",
    "postinstall": "blitz prisma generate && blitz codegen && NODE_ENV=production blitz prisma migrate deploy",
    "migrate": "blitz prisma migrate dev --name init"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-beta.33",
    "@blitzjs/next": "2.0.0-beta.33",
    "@blitzjs/rpc": "2.0.0-beta.33",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "3.3.1",
    "@mantine/core": "7.0.0",
    "@mantine/dates": "7.0.0",
    "@mantine/form": "7.0.0",
    "@mantine/hooks": "7.0.0",
    "@mantine/modals": "7.0.0",
    "@mantine/notifications": "7.0.0",
    "@mantine/nprogress": "7.0.0",
    "@mantine/vanilla-extract": "7.0.0",
    "@prisma/client": "5.3.1",
    "@tabler/icons-react": "2.34.0",
    "blitz": "2.0.0-beta.33",
    "dayjs": "1.11.10",
    "next": "13.4.5",
    "prisma": "5.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.1",
    "resend": "1.1.0",
    "secure-password": "4.0.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.1",
    "@next/env": "13.5.1",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "20.6.3",
    "@types/preview-email": "3.0.1",
    "@types/react": "18.2.22",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@vitejs/plugin-react": "4.0.4",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "14.0.1",
    "postcss": "8.4.30",
    "postcss-preset-mantine": "1.7.0",
    "postcss-simple-vars": "7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "5.0.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.19",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.4"
  },
  "private": true
}
